varnishtest "Big body"

server s1 {
	rxreq
	txresp -bodylen "513"
} -start

varnish v1 -arg "-p thread_pool_stack=92k" -vcl+backend {
	include "${pwd}/waf.vcl";

	sub vcl_init {
		varnish_waf.add_files("${testdir}/test.conf");
	}

} -start

client c1 {
	txreq
	rxresp
	expect resp.status == 200
} -run
