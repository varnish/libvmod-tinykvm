varnishtest ""

server s1 {
	rxreq
	txresp
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
} -start

client c1 {
	send "GET / HTTP/1.1\r\nHost: asdf.com\r\nFoo: bar\r\n\r\n\r\n\r\n\r\n"
	rxresp
	send "GET / HTTP/1.1\r\nHost: asdf.com\r\nFOO: bar\r\n\r\n"
	delay 1
	rxresp
	send "GET / HTTP/1.1\r\nHost: asdf.com\r\n\r\n"
	rxresp
} -run
